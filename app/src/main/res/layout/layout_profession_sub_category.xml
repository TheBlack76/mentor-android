<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:paddingBottom="@dimen/fragment_medium_margin"
    android:layout_marginEnd="@dimen/fragment_medium_margin"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <com.google.android.material.button.MaterialButton
        android:id="@+id/btnName"
        android:layout_width="wrap_content"
        android:layout_height="38dp"
        android:backgroundTint="@color/colorPrimaryPeach"
        android:fontFamily="@font/font_roboto_medium"
        android:insetTop="0dp"
        android:insetBottom="0dp"
        android:stateListAnimator="@null"
        android:text="@string/st_upcoming_bookings"
        android:paddingStart="@dimen/body_margin"
        android:paddingEnd="@dimen/body_margin"
        android:textAppearance="@style/TextAppearance.AppCompat.Caption"
        android:textColor="@color/colorPrimaryText"
        app:cornerRadius="8dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:strokeColor="@color/colorPrimary"
        app:strokeWidth="@dimen/stroke_width_small" />

</androidx.constraintlayout.widget.ConstraintLayout>