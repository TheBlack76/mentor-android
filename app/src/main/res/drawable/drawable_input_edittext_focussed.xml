<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape>
            <solid android:color="@color/colorTransparent" />
            <stroke
                android:width="0.7dp"
                android:color="@color/colorPrimary" />

            <corners android:radius="8dp" />
